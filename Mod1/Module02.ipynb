    "import math\n",
    "\n",
    "n = 20\n",
    "x = 10\n",
    "x_fac = (math.factorial(20))\n",
    "n_fac = (math.factorial(n-x))\n",
    "\n",
    "print(0.5**10)\n",
    "\n",
    "prob = ((x_fac)/(n_fac))*((0.5)**10)*((0.5)**10)\n",
    "print(prob)"
   ]
